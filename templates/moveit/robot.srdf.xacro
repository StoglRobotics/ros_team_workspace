<robot xmlns:xacro="http://wiki.ros.org/xacro" name="$ROBOT_NAME$">

  <xacro:arg name="prefix" default="" />

  <xacro:include filename="$(find $PKG_NAME$)/srdf/$ROBOT_NAME$_macro.srdf.xacro"/>

  <xacro:$ROBOT_NAME$_srdf prefix="$(arg prefix)"/>

</robot>
