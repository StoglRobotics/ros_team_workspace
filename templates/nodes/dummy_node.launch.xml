<launch>
  <arg name="runtime_config_package"
       default="dummy_package_namespace"
       description="Package with runtime configurations for the dummy_node node."/>
  <arg name="config_file"
       default="dummy_node"
       description="Configuration file name for the dummy_node node. Without `.yaml` extension."/>

  <node pkg="dummy_package_namespace" exec="dummy_node" output="both">
    <param from="$(find-pkg-share $(var runtime_config_package))/config/$(var config_file).yaml" />
  </node>

</launch>
